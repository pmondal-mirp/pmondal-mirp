#include <iostream>
using namespace std;
int main() {
int x[5]={1,2,3,4,5};
	for(int i=0; i<5; i++)
	cout<<x[i]<<" ";

	char name[50]="my_name";
	for(int i=0; i<7; i++) {
		cout<<name[i];
		}
	cout<<"Enter name: ";
	cin>>name;
       
        cout<<sizeof(name);

	for(int i=0; i<int(sizeof(name)); i++)
	cout<<name[i]<<i;
cout<<endl;
return 0;
}
	
